{
  "name": "<%= appname %>",
  "version": "<%= version %>",
  "description": "<%= description %>",
  "main": "main.js",
  "scripts": {
    "analyze": "webpack --config build/webpack.config.analyze.js",
    "webpack-dev": "webpack-dev-server --config build/webpack.config.dev.js --progress --inline",
    "build": "webpack --config build/webpack.config.prod.js",
    "node-dev": "cross-env NODE_ENV=development supervisor -w ./server,./extensions,./config -e js,hbs ./server/app.js",
    "start": "cross-env NODE_ENV=production UV_THREADPOOL_SIZE=10 node ./server/app.js",
    "dev": "npm run webpack-dev & npm run node-dev",
    "prod": "npm run build && npm run start"
  },
  "keywords": [],
  "author": "<%= author %>",
  "license": "ISC",
  "dependencies": {
    "babel-core": "^6.26.3",
    "handlebars": "^4.0.11",
    "koa": "^2.5.1",
    "koa-handlebars": "^1.0.0",
    "koa-router": "^7.4.0",
    "koa-views": "^6.1.4",
    "mini-logger": "^1.1.3",
    "node-fetch": "^2.1.2",
    "vue": "^2.5.2",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "clean-webpack-plugin": "^0.1.19",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.11",
    "file-loader": "^1.1.11",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.1",
    "vue-loader": "^15.2.6",
    "vue-template-compiler": "^2.5.2",
    "vue-html-loader": "^1.2.4",
    "postcss-loader": "^2.1.5",
    "less": "^3.8.0",
    "less-loader": "^4.1.0",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "supervisor": "^0.12.0",
    "webpack": "^4.2.0",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4",
    "webpack-manifest-plugin": "^2.0.3"
  }
}
